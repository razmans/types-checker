{
  "name": "@razmans/types-checker",
  "version": "1.0.4",
  "description": "A CLI to detect duplicate TypeScript interfaces in a project",
  "types": "dist/index.d.ts",
  "bin": {
    "types-checker": "dist/index.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "test": "npm run test:detected && npm run test:not-detected",
    "test:detected": "node dist/index.js ./test/detected",
    "test:not-detected": "node dist/index.js ./test/not-detected"
  },
  "keywords": [
    "typescript",
    "cli",
    "interfaces",
    "refactoring"
  ],
  "author": "Razman <https://x.com/@razmans>",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.0",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@types/node": "^24.3.0",
    "semantic-release": "^24.2.7",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/razmans/types-duplicate-finder.git"
  }
}
